min_max1(List, Min, Max), List = [First|Rest] =>
    Mn = First,
    Mx = First,
    foreach (Value in Rest)
        if Value < Mn then
            Mn := Value
        elseif Value > Mx then
            Mx := Value
        end
    end,
    Min = Mn,
    Max = Mx.

min_max2(List, Min, Max), List !== [] =>
    Min = min(List),
    Max = max(List).    Max = max(List).
